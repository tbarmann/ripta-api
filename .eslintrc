{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb-base/legacy",
  "globals": {
    "$": false,
    "opts": true,
    "Spinner": false,
    "readCookie": true,
    "createCookie": true,
    "moment": false,
    "_": false
  },
  "rules": {
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "never"
    }],
    "func-names": 0,
    "no-prototype-builtins": 0,
    "object-curly-spacing": 0,
    "no-use-before-define": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
