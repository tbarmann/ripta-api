{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb-base/legacy",
  "globals": {
    "$": false,
    "opts": false,
    "Spinner": false,
    "readCookie": false,
    "createCookie": false,
    "moment": false,
    "_": false
  },
  rules: {
    "space-before-function-paren": ["error", {
      "anonymous": "never",
      "named": "never",
      "asyncArrow": "never"
    }],
    "func-names": 0,
    "no-prototype-builtins": 0,
    "object-curly-spacing": 0,
    "no-use-before-define": 0

  }
}
